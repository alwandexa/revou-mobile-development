{
  "presets": ["module:@react-native/babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@contexts": "./src/contexts",
          "@screens": "./src/screens",
          "@utils": "./src/utils",
          "@services": "./src/services"
        }
      }
    ]
  ]
}
